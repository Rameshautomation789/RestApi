package JavaBascis;

public class ExceptionHandling {

	
	public static void main(String[] args) throws InterruptedException {
		
		
		// uncaught Exception
//		int i=9/0;
//		System.out.println(i);
		
		//caught Exception
		
	//	Thread.sleep(2000);
		

		
	//	1. try catch 
		
		try{
			int i=9/0;
		}catch(Exception e){
			e.printStackTrace();
			e.getMessage();
		}
		
		System.out.println("ABC");
		System.out.println("ABC");
		System.out.println("ABC");
		
		
	}

}
